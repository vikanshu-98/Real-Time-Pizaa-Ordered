
{{!-- {{ orderData }} --}}

<section class="singleOrder">
    <div class="custom-container mx-auto">
        <div class="status-box w-2/3 mx-auto">
            <div class="flex items-center justify-between mb-12">
                <h1 class="text-xl font-bold">Track delivery status</h1>
                <h6 class="bg-white py-1 px-1 rounded-full text-green-600 text-xs">62b064a284a621824f31b156</h6>
            </div>
            <input type="hidden" id="singleOrderData" value="{{JSON2String  orderData}}" />
            <ul>
                <li class="status_line text-xl pb-16" data-status="order_placed"><span> Order Place</span></li>
                <li class="status_line text-xl pb-16" data-status="confirmed"><span> Order  Confirmation</span></li>
                <li class="status_line text-xl pb-16" data-status="prepared"><span>Order Preparation</span></li>
                <li class="status_line text-xl pb-16" data-status="delivered"><span>Out for delivery</span></li>
                <li class="status_line text-xl" data-status="completed"><span>Complete</span></li>
            </ul>
        </div>
    </div>
</section>